---
import cn from "src/utils/cn";

export interface Props {
  className: string;
}

const { className } = Astro.props;
---

<section
  class={cn(
    "w-full h-full flex flex-col items-center justify-center bg-[url('../assets/background/usecase_content_vector.svg')] bg-no-repeat bg-right bg-auto",
    className,
  )}
>
  <slot />
</section>
