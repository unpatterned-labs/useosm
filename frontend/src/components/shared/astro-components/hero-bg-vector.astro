---
import cn from "src/utils/cn";

export interface Props {
  className: string;
}

const { className } = Astro.props;
---

<section
  class={cn(
    "w-full flex flex-col items-center justify-center sm:bg-[url('../assets/background/home_vector.svg')] bg-[url('../assets/background/sm_home_vector.svg')] bg-no-repeat bg-cover",
    className,
  )}
>
  <slot />
</section>
