{
  "name": "front-end",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier . --write",
    "format:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,astro,json,css,scss,md,mdx,test.ts}'",
    "lint": "eslint .",
    "precommit": "lint-staged",
    "prepare": "cd .. && husky frontend/.husky"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,astro,json,css,scss,md,mdx,test.ts}": [
      "pnpm format",
      "pnpm test",
      "pnpm lint"
    ]
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.12",
    "@astrojs/react": "^4.4.2",
    "@fontsource-variable/geist": "^5.2.8",
    "@headlessui/react": "^2.2.9",
    "@tailwindcss/vite": "^4.1.12",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@vis.gl/react-maplibre": "^8.1.0",
    "astro": "^5.16.2",
    "clsx": "^2.1.1",
    "eslint": "^9.39.2",
    "gsap": "^3.13.0",
    "maplibre-gl": "^5.9.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-class-names": "^2.0.0",
    "rehype-slug": "^6.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-astro": "^1.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.6.14",
    "typescript-eslint": "^8.53.0"
  }
}
